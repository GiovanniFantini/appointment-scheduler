  "CorsOrigins": [
    "https://appointment-consumer-app.azurewebsites.net",
    "https://appointment-merchant-app.azurewebsites.net",
    "https://appointment-employee-app.azurewebsites.net",
    "https://appointment-admin-app.azurewebsites.net"
